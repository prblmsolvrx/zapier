generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
   id         Int @default(autoincreament())
   name       String
   email      String
   password   String
}

model Zap {
    id String @default(uuid())   
}

model Trigger{
  id   String @default(uuid())
}

